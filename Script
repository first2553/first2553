local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("First hub", "Midnight")
 
--Asenal
 
local Tab = Window:NewTab("Asenal")
local Section = Tab:NewSection("Asenal")
 
Section:NewButton("Script", "ButtonInfo", function()
    loadstring(game:HttpGet(('https://pastebin.com/raw/kvpwyQRT')))()
end)
 
--MOKORO HUB
 
local Tab = Window:NewTab("MOKORO HUB")
local Section = Tab:NewSection("MOKORO HUB")
 
Section:NewButton("Script", "ButtonInfo", function()
    loadstring(game:HttpGet"https://raw.githubusercontent.com/xDepressionx/Free-Script/main/AllScript.lua")()
end)
 
--Stike Hub
 
local Tab = Window:NewTab("Stike Hub")
local Section = Tab:NewSection("Stike Hub")
 
